<h1>Display your Pirate here</h1>
<ul>
<li>Name:<%= @pirate.name %></li>
<li>Weight:<%= @pirate.weight %>pounds</li>
<li>Height:<%= @pirate.height %>inches</li>
<!-- # <% end %> -->
</ul>
<h2>Display your first ship here</h2>
<ul>
<% @ships.each do |ship| %>
<li>Name:<%= ship.name %></li>
<li>Type:<%= ship.type %></li>
<li>Booty:<%= ship.booty %></li>
<!-- # <% end %> -->
</ul>



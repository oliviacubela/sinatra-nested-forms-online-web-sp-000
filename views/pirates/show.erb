<h1>Display your Pirate here</h1>
<h1>Pirate</h1>
<div class="pirate">
<p>Name:<%= @pirate.name %></p><br>
<p>Weight:<%= @pirate.weight %>pounds</p><br>
<p>Height:<%= @pirate.height %>inches</p><br>
</div><br>

<h2>Display your first ship here</h2>
<h1>Ships</h1>
<% @ships.each do |ship| %>
<div class="ship">
<p>Name:<%= ship.name %></p>
<p>Type:<%= ship.type %></p>
<p>Booty:<%= ship.booty %></p>
</div><br>
<% end %>
<h2>Display your second ship here</h2>

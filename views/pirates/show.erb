<h1>Display your Pirate here</h1>
<ul>
<li><%= @pirate.name %></li>
<li><%= @pirate.weight %></li>
<li><%= @pirate.height %></li>
<% end %>
</ul>
<h2>Display your first ship here</h2>
<ul>
<% @ships.each do |ship| %>
<li><%= ship.name %></li>
<li><%= ship.type %></li>
<li><%= ship.booty %></li>
<% end %>
</ul>


